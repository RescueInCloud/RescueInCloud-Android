DROP TABLE IF EXISTS "farmacos";
CREATE TABLE farmacos (id_farmaco INTEGER PRIMARY KEY, nombre_farmaco TEXT, nombre_fabricante TEXT, presentacion_farmaco TEXT, tipo_administracion TEXT, descripcion_farmaco TEXT);
INSERT INTO "farmacos" VALUES(1,'aspirina','bayer','500g','comprimidos','unas pastis');
INSERT INTO "farmacos" VALUES(2,'diazepam','bayer','sobres','oal','unas pastis');
INSERT INTO "farmacos" VALUES(3,'paracetamol','un fabricante','500mg','oral','un calmante');
DROP TABLE IF EXISTS "notas";
CREATE TABLE notas (nombre_nota TEXT PRIMARY KEY, descripcion TEXT);
DROP TABLE IF EXISTS "papito";
CREATE TABLE "papito" ("kk" DOUBLE PRIMARY KEY  NOT NULL , "jui" INTEGER);
DROP TABLE IF EXISTS "protocolos";
CREATE TABLE protocolos (protocolos_id INTEGER PRIMARY KEY AUTOINCREMENT, nombre_protocolo TEXT);
