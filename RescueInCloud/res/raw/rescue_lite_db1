DROP TABLE IF EXISTS "caja_texto_hijos";
CREATE TABLE "caja_texto_hijos" (id INTEGER PRIMARY KEY AUTOINCREMENT, caja_texto_hijo INTEGER, caja_texto_padre INTEGER, tipo_relacion TEXT, id_protocolo INTEGER);
